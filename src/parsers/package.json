{
  "scripts": {
    "start": "tsc && node dist/main",
    "lint": "eslint --ext .js,.jsx,ts,.tsx .",
    "test": "jest"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^5.59.9",
    "@typescript-eslint/types": "^5.59.9",
    "filbert": "^0.1.20",
    "java-parser": "^2.0.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^18.15.5",
    "eslint": "^8.42.0",
    "eslint-config-kyle": "^11.14.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "~5.0"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "dist"
    ],
    "extends": "kyle",
    "rules": {
      "unicorn/no-abusive-eslint-disable": 0,
      "class-methods-use-this": 0,
      "no-param-reassign": 0
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
