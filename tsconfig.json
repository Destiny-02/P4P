{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "outDir": "dist",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "strict": true,
    "downlevelIteration": true,
    "lib": ["esnext", "dom"] // dom is to hack around https://github.com/DefinitelyTyped/DefinitelyTyped/issues/60924
  },
  "include": ["**/*"],
  "exclude": ["dist", "src/donotcommit", "data"]
}
